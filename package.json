    "node": ">= 20.11.0",
    "npm": ">= 10.8.1"
  },
  "type": "module",
  "scripts": {
    "dev": "vitepress dev docs",
    "preview": "vitepress preview docs",
    "build": "vitepress build docs",
    "lint:js": "eslint --ext .js,.ts .",
    "lint:js:fix": "eslint --fix --ext .js,.ts .",
    "lint:files": "prettier --check \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:files:fix": "prettier --write \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:markdown": "markdownlint **/*.md",
    "lint:full": "npm run lint:js && npm run lint:files && npm run lint:markdown",
    "lint:full:fix": "npm run lint:js:fix && npm run lint:files:fix && npm run lint:markdown"
  },
  "devDependencies": {
    "@types/node": "^20.13.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vite-pwa/vitepress": "^0.5.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "markdownlint": "0.34.0",
    "markdownlint-cli": "0.41.0",
    "prettier": "3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitepress": "1.2.2"
  }
